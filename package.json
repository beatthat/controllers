{
    "name": "controllers",
    "scope": "ape",
    "version": "0.0.5",
    "description": "a non-dogmatic MVC'ish framework for unity",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/beatthat/controllers.git"
    },
    "files": [
        "index.js",
        "scripts",
        "src"
    ],
    "author": "larrykirschner@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/beatthat/controllers/issues"
    },
    "homepage": "https://github.com/beatthat/controllers",
    "scripts": {
        "postinstall": "node scripts/postinstall.js",
        "install:test": "rm -f *.tgz && npm pack && cd test && npm run clean && npm install",
        "clean": "rm -f *.tgz && cd test && npm run clean",
        "cp:test2src": "cd test && node scripts/cp-test2src.js",
        "overwrite:test2src": "cd test && node scripts/overwrite-test2src.js",
        "pub:tag": "git add -A && git commit -m ${npm_package_version} && git tag -a ${npm_package_version} -m ${npm_package_version} && git push origin master && git push origin ${npm_package_version}"
    },
    "dependencies": {
        "path": "^0.12.7",
        "unity-npm-utils": "github:beatthat/unity-npm-utils#semver:^0.0.9",
        "bindings": "beatthat/bindings#semver:^0.0.7",
        "binding-behaviours": "beatthat/binding-behaviours#semver:^0.0.15",
        "commands-core": "beatthat/commands-core#semver:^0.0.6",
        "controller-interfaces": "beatthat/controller-interfaces#semver:^0.0.3",
        "gameobject-utils": "beatthat/gameobject-utils#semver:^0.0.4",
        "notifications": "beatthat/notifications#semver:^0.0.10",
        "placements": "beatthat/placements#semver:^0.0.5",
        "pools": "beatthat/pools#semver:^0.0.8",
        "services": "beatthat/services#semver:^0.0.6",
        "type-utils": "beatthat/type-utils#semver:^0.0.6"
    }
}
